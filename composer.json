{
    "name": "offline-agency/oa-laravel-cart",
    "description": "Laravel shoppingcart with fiscal support",
    "keywords": ["laravel", "shoppingcart", "cart"],
    "license": "MIT",
    "authors": [
        {
            "name": "Offline Agency",
            "email": "info@offlineagency.it"
        }
    ],
    "require": {
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*",
        "illuminate/session": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*",
        "illuminate/events": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0|~6.0|~7.0",
        "mockery/mockery": "~0.9.0",
        "orchestra/testbench": "~3.1"
    },
    "autoload": {
        "psr-4": {
            "Gloudemans\\Shoppingcart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gloudemans\\Tests\\Shoppingcart\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "suggest": {
        "gloudemans/notify": "Simple flash notifications for Laravel"
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "OfflineAgency\\OaLaravelCart\\ShoppingcartServiceProvider"
            ],
            "aliases": {
                "Cart": "OfflineAgency\\OaLaravelCart\\Facades\\Cart"
            }
        }
    }
}
